(*<case> ::= case <expr> of { <value> ':' <body> ';' } [ else <body> ';' ] end*)
program simple9;
var
    a, b;
begin
    readln(a);
    case a of
        3 :
            begin
                writeln('it's 3')
            end;
        2.0 :
            begin
                writeln('so 2.0')
            end;
        'morango' :
            begin
                writeln('there's morango')
            end;
        else 
            begin
               writeln('sorry, there's no match') 
            end;
    end
end.